    <script>
        function recomment() {
            layui.use('layer', function(){
                var layer = layui.layer;
                layer.open({
                    offset: ['30%', '40%'],
                    title:'投推荐票',
                    content:'<input class="recommentTieckts" type="number" placeholder="选择要投的票数">',
                    btn:['确认','取消'],
                    yes:function () {
                        $.ajax({
                            url:'<%=basePath%>reader/recommentTieckts.action?num='+$(".recommentTieckts").val(),
                            async:true,
                            success:function (flag) {
                                if(flag){
                                    layui.use('layer', function(){
                                        var layer = layui.layer;
                                        layer.msg('感谢您的推荐')
                                    })
                                }else{
                                    layui.use('layer', function(){
                                        var layer = layui.layer;
                                        layer.msg('不好意思，投推荐票失败了')
                                    })
                                }
                            }
                        })
                    }
                });
            })
        }

    </script>
