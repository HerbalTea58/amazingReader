

.head{
    width: auto;
    height: 100px;
    background: #FFFDFD;
    margin-left: -10px;
    margin-right: -10px;
    position: relative;
    border-bottom: #dddddd 1px solid;
    /*border: 1px #000 solid;*/
}

.title {
    width: 700px;
    height: 80px;
    float:left;
    margin-left: 70px;
    margin-top: 15px;
    /*border: 1px #000 solid;*/
}
.title a:link{
    text-decoration: none;
    color: #2aabd2;
}
.title a:hover{
    text-decoration: none;
    color: #2aabd2;
}
.title a:visited{
    text-decoration: none;
    color: #2aabd2;
}
.logo{
    width: 200px;
    height: 80px;
    float: left;
}
.title1{
    font-family: 楷体;
    font-size: 50px;
}
.title2{
    font-family: 华文行楷;
    font-size: 70px;
/*    border: #0f0f0f 1px ;
float: left;
margin-left: 20px;*/
}
.notify{
    width: 200px;
    height: 80px;
    float:right;
    margin-right: 80px;
    margin-top: 10px;
    /*border: 1px #000 solid;*/
    font-family: 楷体;
    font-size: 20px;
}
.img1{
    width: 50px;
    height: 50px;
    float: left;
    margin-left: 20px;
    margin-top: 20px;
    border-radius: 50%;
    /*border: 1px #000 solid;*/
}
.notify1{
    width: 80px;
    height: 30px;
    float: left;
    margin-left: 10px;
    margin-top: 35px;
    /*border: 1px #000 solid;*/
}
.notify1 a:link{
    text-decoration: none;
    color: #000000;
}
.notify1 a:hover{
    text-decoration: none;
    color: #000000;
}
.notify1 a:visited{
    text-decoration: none;
    color: #000000;
}
.float_div{
    width: 150px;
    height: 90px;
    position: absolute;
    right: 120px;
    top: 100px;
    background-color: white;
    border: #dddddd 1px solid;
    display: none;
    z-index: 10;
}
.arrow{
    width: 10px;
    height: 10px;
    position: absolute;
    left:70px;
    top:-6px;
    transform:rotate(45deg);
    background-color: white;
    border-top: #dddddd 1px solid;
    border-left: #dddddd 1px solid;
    /*border: #0f0f0f 1px solid;*/
}
.float_span{
    width: 110px;
    height: 30px;
    line-height: 30px;
    /*text-align: center;*/
    font-size: 15px;
    font-family: 楷体;
    float: left;
    margin-left: 20px;
    border-bottom: #dddddd 1px solid;
    /*border: #0f0f0f 1px solid;*/
}



.main{
    width: 1180px;
    height: 1000px;
    float: left;
    margin-top: 25px;
    margin-left: 70px;
    background: #ffffff;
    border: #dddddd 1px solid;
}
