/* 公共样式 */
.content{width: 1000px; overflow: hidden; margin: 0 auto; }
.content a{color: #212121;}
.content a:hover{color:#f26552;}
.content s{width: 24px; height: 27px; float: left; background: url(../Images/rankicon.png) 0px -96px no-repeat;}
/* 大标题 */
.bigTitle{width: 166px; height: 27px; line-height: 27px; padding: 46px 0 24px 0; margin: 0 auto;}
.bigTitle h2{float: right; font-size: 26px; color:#212121;}

/* 榜单 */
.rankList{background: #fff;}
/* 小标器 */
.littleTitle{height: 104px; border-bottom: 1px solid #f0f0f0;}
.littleTitle .dif_Area{width: 440px; padding-top: 31px; text-align: center; float: left;}
.littleTitle .dif_Area_Head{width: 53px; height: 16px; line-height: 16px; font-size: 16px; margin: 0 auto 12px;}
.littleTitle .dif_Area_Head s{width: 14px; height: 16px; background-position: -28px -97px; margin-right: 6px;}
.littleTitle .dif_Area_Head .man{width: 15px; height: 16px; background-position: 0 -126px;}
.littleTitle .dif_Area_Head .woman{width: 15px; height: 16px; background-position: -17px -126px;}
.littleTitle p{font-size: 14px;}
.littleTitle p a{float: left;}
.littleTitle p b{margin: -1px 9px 0; float: left;}
.littleTitle .dif_Area .selected{color:#f26552;}
/* 榜单列表 */
.rankListCon{padding: 14px 30px 12px 30px ; overflow: hidden;}
.rankListCon ul{overflow: hidden;}
.rankListCon ul li{ padding: 0 16px; height: 40px; line-height: 40px; font-size: 14px;}
.rankListCon ul .fisRow{background: #f7f7f7; margin-bottom: 12px;  color:#9e9e9e; font-size: 12px;}
.rankListCon ul li p{float: left;  text-align: left; }
.rankListCon ul li .firCol{width: 57px;}
.rankListCon ul li .secCol{width: 382px; overflow: hidden;  white-space: nowrap;  text-overflow: ellipsis; padding-right: 10px; }
.rankListCon ul li .thiCol{width: 220px; overflow: hidden;  white-space: nowrap;  text-overflow: ellipsis;}
.rankListCon ul li .fouCol{width: 175px;}
.rankListCon ul li .fivCol{width: 58px; text-align: right;}


.rankListCon .load{margin: 28px auto; width: 110px; height: 32px; line-height: 32px; font-size: 14px; color: #9e9e9e; }
.rankListCon .load img{float: left; margin-right: 8px;}


