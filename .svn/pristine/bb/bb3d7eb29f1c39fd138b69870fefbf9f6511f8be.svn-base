package com.amazingReader.entity.utilEntity;

import java.util.Date;

public class WriterOrder {
    private Integer readerOrderId;

    private String readerNeckName;

    private String orderTypeName;

    private Integer readerOrderPrice;

    private String bookName;

    private Date readerOrderTime;

    public WriterOrder() {
    }

    public Integer getReaderOrderId() {
        return readerOrderId;
    }

    public void setReaderOrderId(Integer readerOrderId) {
        this.readerOrderId = readerOrderId;
    }

    public String getReaderNeckName() {
        return readerNeckName;
    }

    public void setReaderNeckName(String readerNeckName) {
        this.readerNeckName = readerNeckName;
    }

    public String getOrderTypeName() {
        return orderTypeName;
    }

    public void setOrderTypeName(String orderTypeName) {
        this.orderTypeName = orderTypeName;
    }

    public Integer getReaderOrderPrice() {
        return readerOrderPrice;
    }

    public void setReaderOrderPrice(Integer readerOrderPrice) {
        this.readerOrderPrice = readerOrderPrice;
    }

    public String getBookName() {
        return bookName;
    }

    public void setBookName(String bookName) {
        this.bookName = bookName;
    }

    public Date getReaderOrderTime() {
        return readerOrderTime;
    }

    public void setReaderOrderTime(Date readerOrderTime) {
        this.readerOrderTime = readerOrderTime;
    }

    @Override
    public String toString() {
        return "WriterOrder{" +
                "readerOrderId=" + readerOrderId +
                ", readerNeckName='" + readerNeckName + '\'' +
                ", orderTypeName='" + orderTypeName + '\'' +
                ", readerOrderPrice=" + readerOrderPrice +
                ", bookName='" + bookName + '\'' +
                ", readerOrderTime=" + readerOrderTime +
                '}';
    }
}
